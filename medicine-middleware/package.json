    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.4.3",
    "@nestjs/sequelize": "^9.0.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.8",
    "base-64": "^1.0.0",
    "hl7": "^1.1.1",
    "hl7v2": "^0.14.0",
    "nodemailer": "^7.0.0",
    "pg": "^8.15.6",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sequelize": "^6.37.7",
